{
  "rangeStrategy": "widen",
  "extends": ["config:base", ":enablePreCommit"],
  "schedule": ["after 10pm and before 5am every weekday", "every weekend"],
  "semanticCommits": "enabled",
  "lockFileMaintenance": { "enabled": true },
  "enabledManagers": [
    "docker-compose",
    "dockerfile",
    "github-actions",
    "poetry"
  ],
  "automerge": false,
  "labels": ["dependencies"]
}
